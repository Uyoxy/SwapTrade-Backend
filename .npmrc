# Avoid idle timeout on slow or flaky networks (default 300000 ms can still hit EIDLETIMEOUT)
fetch-timeout=120000
fetch-retries=5

# Allow install on Node 25 until opossum lists it (optional; npm only warns by default)
engine-strict=false

# Use legacy peer dependency resolution (avoids conflicts; installs more reliably)
legacy-peer-deps=true
